    http://www.springframework.org/schema/context/spring-context.xsd">
        <bean name="person" class="cogent.infotech.com.autowiring_annotation.Person" ></bean>
        <bean name="car" class="cogent.infotech.com.autowiring_annotation.Car" p:model="Honda" p:price="10000" ></bean>
        <bean class="org.springframework.beans.factory.annotation.Autowired"></bean>
        <context:annotation-config></context:annotation-config>

